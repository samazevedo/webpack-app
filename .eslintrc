{
	"extends": ["mdcs", "prettier"],
	"plugins": ["@typescript-eslint", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"env": { "node": true, "es6": true, "browser": true },
	"parser": "@typescript-eslint/parser",
	"rules": {
		"no-console": "off",
		"array-bracket-spacing": ["error", "never"]
		// "prettier/prettier": 2
	}
}
